    "concurrently": "^3.5.0",
    "electron": "~1.6.2",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.6",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.6.0"
